{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://lheyberger.github.io/mtg-json-schema/schemas/commander-0.0.1.json",
    "title": "commander-0.0.1.json",
    "description": "A single commander deck",
    "type": "object",
    "allOf": [
        {
            "$ref": "https://lheyberger.github.io/mtg-json-schema/schemas/deck-0.0.1.json"
        },
        {
            "required": [
                "version"
            ],
            "properties": {
                "version": {
                    "$id": "#/properties/version",
                    "type": "string",
                    "default": "",
                    "pattern": "^commander-[0-9]+\\.[0-9]+\\.[0-9]+$"
                }
            }
        }
    ],
    "additionalProperties": true
}
